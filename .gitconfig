[core]
  repositoryformatversion = 0
  filemode = false
  bare = false
  logallrefupdates = true
  symlinks = false
  ignorecase = true
  hideDotFiles = dotGitOnly

[merge]
  tool = unityyamlmerge

[mergetool "unityyamlmerge"]
  trustExitCode = false
  keepTemporaries = true
  keepBackup = false
  path = 'C:\\Program Files\\Unity\\Editor\\Data\\Tools\\UnityYAMLMerge.exe'
  cmd = 'C:\\Program Files\\Unity\\Editor\\Data\\Tools\\UnityYAMLMerge.exe' merge -p "$BASE" "$REMOTE" "$LOCAL" "$MERGED"

[diff "unity"]
      textconv='C:\\Users\\Nick Pampe\\unityYamlTextConv.py'

[remote "origin"]
  url = git@github.com:NicoPampe/GameDevSandbox.git
  fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
  remote = origin
  merge = refs/heads/master
[branch "feature/playerBehavior"]
  remote = origin
  merge = refs/heads/feature/playerBehavior
[branch "feature/AiBehavior"]
  remote = origin
  merge = refs/heads/feature/AiBehavior
[branch "feature/lvlSettup"]
  remote = origin
  merge = refs/heads/feature/lvlSettup
